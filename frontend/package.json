{
  "name": "chess",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.1",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "@types/websocket": "^1.0.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.25.8",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.8",
    "vue-tsc": "^1.0.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue3-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "jsdom"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  }
}
